{
  "expo": {
    "sdkVersion": "37.0.0",
    "name": "fitness",
    "description": "The best triathlon tracker ever",
    "slug": "fitness",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/images/icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.fitness.exp"
    },
    "android": {
      "package": "org.fitness.exp"
    }
  }
}
